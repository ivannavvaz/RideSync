<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.inavarro.ridesync.mainModule.shopModule.ShopFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_shop" >
        <action
            android:id="@+id/action_homeFragment_to_chatsFragment2"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_findFragment2"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/findFragment"
        android:name="com.inavarro.ridesync.mainModule.searchModule.SearchFragment"
        android:label="fragment_find"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_findFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_findFragment_to_chatsFragment2"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_findFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_findFragment_to_activityFragment"
            app:destination="@id/activityFragment" />
    </fragment>
    <fragment
        android:id="@+id/GroupsFragment"
        android:name="com.inavarro.ridesync.mainModule.groupsModule.GroupsFragment"
        android:label="fragment_chats"
        tools:layout="@layout/fragment_groups" >
        <action
            android:id="@+id/action_chatsFragment_to_findFragment2"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_chatsFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_chatsFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_chatsFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.inavarro.ridesync.mainModule.profileModule.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_findFragment2"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_profileFragment_to_chatsFragment2"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_homeFragment2"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.inavarro.ridesync.mainModule.chatModule.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" >
        <action
            android:id="@+id/action_chatFragment_to_chatsFragment"
            app:destination="@id/GroupsFragment" />
        <argument
            android:name="idGroup"
            app:argType="string"
            android:defaultValue="0" />
        <argument android:name="nameGroup"
            app:argType="string"
            android:defaultValue='""' />
        <action
            android:id="@+id/action_chatFragment_to_infoChatFragment"
            app:destination="@id/infoChatFragment" />
    </fragment>
    <fragment
        android:id="@+id/myGroupsFragment"
        android:name="com.inavarro.ridesync.mainModule.groupsModule.myGroups.MyGroupsFragment"
        android:label="fragment_my_groups"
        tools:layout="@layout/fragment_my_groups" >
        <action
            android:id="@+id/action_myGroupsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_myGroupsFragment_to_findFragment"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_myGroupsFragment_to_GroupsFragment"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_myGroupsFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_myGroupsFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_myGroupsFragment_to_profileFragment3"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchGroupsFragment"
        android:name="com.inavarro.ridesync.mainModule.groupsModule.searchGroups.SearchGroupsFragment"
        android:label="SearchGroupsFragment" >
        <action
            android:id="@+id/action_searchGroupsFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_searchGroupsFragment_to_findFragment"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_searchGroupsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_searchGroupsFragment_to_GroupsFragment"
            app:destination="@id/GroupsFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.inavarro.ridesync.mainModule.editProfileModule.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <action
            android:id="@+id/action_editProfileFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoChatFragment"
        android:name="com.inavarro.ridesync.mainModule.infoChatModule.InfoChatFragment"
        android:label="fragment_info_chat"
        tools:layout="@layout/fragment_info_chat" >
        <action
            android:id="@+id/action_infoChatFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <argument
            android:name="idGroup"
            app:argType="string"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_infoChatFragment_to_GroupsFragment"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_infoChatFragment_to_viewProfileFragment"
            app:destination="@id/viewProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewProfileFragment"
        android:name="com.inavarro.ridesync.mainModule.viewProfileModule.ViewProfileFragment"
        android:label="fragment_view_profile"
        tools:layout="@layout/fragment_view_profile" >
        <action
            android:id="@+id/action_viewProfileFragment_to_infoChatFragment"
            app:destination="@id/infoChatFragment" />
        <argument
            android:name="idUser"
            app:argType="string"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/activityFragment"
        android:name="com.inavarro.ridesync.mainModule.activityModule.ActivityFragment"
        android:label="fragment_activity"
        tools:layout="@layout/fragment_activity" >
        <action
            android:id="@+id/action_activityFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_activityFragment_to_findFragment"
            app:destination="@id/findFragment" />
        <action
            android:id="@+id/action_activityFragment_to_GroupsFragment"
            app:destination="@id/GroupsFragment" />
        <action
            android:id="@+id/action_activityFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
</navigation>